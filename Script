class User {
    constructor (id, firstname, lastname, email, age, photo) {
    this.id = id;
    this.firstname = firstname;
    this.lastname = lastname;
    this.email = email;
    this.age = age;
    this.photo = photo;
    }
   }

class ProductsScript {
    constructor(id, title, description, price, brand, category, photo) {
        this.id = id;
        this.title = title;
        this.description = description;
        this.price = price;
        this.brand = brand;
        this.category = category;
        this.photo = photo; 
    }
}